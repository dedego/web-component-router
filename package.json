    "example": "es-dev-server --watch --node-resolve --open --app-index index.html"
  },
  "repository": "github:dedego/web-component-router",
  "keywords": [
    "web components",
    "router"
  ],
  "author": "Dennis de Goede",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dedego/web-component-router/issues"
  },
  "homepage": "https://github.com/dedego/web-component-router#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^24.9.0",
    "es-dev-server": "^1.38.3",
    "jest": "^24.9.0",
    "lit-element": "^2.2.1",
    "rollup": "^1.31.0",
    "rollup-plugin-terser": "^5.2.0"
  },
  "browserslist": [
    "defaults"
  ],
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
